{
  "name": "bridge",
  "packageManager": "yarn@3.2.2",
  "main": "index.ts",
  "version": "1.2.2",
  "license": "CC BY-NC-SA 4.0",
  "private": true,
  "scripts": {
    "start": "ts-node --transpileOnly .",
    "dev": "ts-node .",
    "server": "cd server && java -jar server.jar nogui",
    "lint": "eslint --fix .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "discord.js": "^14.0.3",
    "dotenv": "^16.0.1",
    "mineflayer": "^4.3.0",
    "prismarine-nbt": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "printWidth": 160,
    "endOfLine": "lf"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ]
  }
}
